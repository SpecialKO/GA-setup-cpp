{
	"problemMatcher": [
		{
			"owner": "gcc",
			"pattern": [
				{
					"regexp": "^([^:]+):.*(\\d+):(\\d+):\\s+(?:fatal )?(warning|error|note):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(.*)\\((\\d+),?(\\d*)\\)\\s*:\\s*(error|warning|message)\\s+(\\w+\\s*:\\s+.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "make",
			"pattern": [
				{
					"regexp": "\\[([^:]+)(?::(\\d+):(.+))?\\]\\s(Error)",
					"file": 1,
					"line": 2,
					"severity": 4,
					"message": 0
				}
			]
		},
		{
			"owner": "configure",
			"pattern": [
				{
					"regexp": "(configure): (error): (.+)",
					"file": 1,
					"severity": 2,
					"message": 3
				}
			]
		}
	]
}
