{
	"problemMatcher": [
		{
			"owner": "gcc",
			"pattern": [
				{
					"regexp": "^([^:]+):(\\d+):(?:(\\d+):)? (?:fatal )?(warning|error|note): (.+?)(?: \\[(.+)\\])?$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 6,
					"message": 5
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(.*)\\((\\d+),?(\\d*)\\)\\s*:\\s*(error|warning|message|note) *(\\w\\d+)?:(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 5,
					"message": 6
				}
			]
		},
		{
			"owner": "make",
			"pattern": [
				{
					"regexp": "\\[([^:]+)(?::(\\d+):(.+))?\\]\\s(Error).+",
					"file": 1,
					"line": 2,
					"severity": 4,
					"message": 0
				}
			]
		},
		{
			"owner": "generic error parser",
			"pattern": [
				{
					"regexp": ".+ (error): (.+)",
					"file": 1,
					"severity": 1,
					"message": 0
				}
			]
		}
	]
}
