{
	"problemMatcher": [
		{
			"owner": "gcc",
			"pattern": [
				{
					"regexp": "^([^:]+):.*(\\d+):(\\d+):\\s+(warning|error|fatal error|note):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(.*)\\((\\d+),?(\\d*)\\)\\s*:\\s*(error|warning|message)\\s+(\\w+\\s*:\\s+.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			]
		}
	]
}
